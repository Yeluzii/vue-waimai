{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Header from '../../components/HeaderOf1.vue';\nimport { useRouter } from 'vue-router';\nimport { reactive, toRefs } from 'vue';\nimport { Toast } from 'vant';\nexport default {\n  components: {\n    Header\n  },\n  setup() {\n    const router = useRouter();\n    const onSubmit = value => {\n      if (!localStorage.userInfo) {\n        Toast('账号未注册！！！');\n        return;\n      } else {\n        let userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\n        if (userInfo['username'] === value['username']) {\n          if (userInfo['password'] === value['password']) {\n            localStorage.setItem(\"isLogin\", 'login');\n            Toast('登录成功!!!');\n            router.push('/home');\n          } else {\n            Toast('密码错误!!!');\n            return;\n          }\n        } else {\n          Toast('账号不存在!!!');\n          return;\n        }\n      }\n    };\n    const toRegister = () => {\n      router.push('/register');\n    };\n    const data = reactive({\n      username: '',\n      password: ''\n    });\n    return {\n      onSubmit,\n      toRegister,\n      ...toRefs(data)\n    };\n  }\n};","map":{"version":3,"names":["Header","useRouter","reactive","toRefs","Toast","components","setup","router","onSubmit","value","localStorage","userInfo","JSON","parse","getItem","setItem","push","toRegister","data","username","password"],"sources":["D:\\vue3-projects\\vue-waimai\\src\\views\\login\\LoginIndex.vue"],"sourcesContent":["<template>\r\n    <div class=\"login\">\r\n        <Header title=\"登录\" />\r\n        <div class=\"img\">也团</div>\r\n        <van-form @submit=\"onSubmit\">\r\n            <van-field v-model=\"username\" name=\"username\" label=\"用户名\" placeholder=\"用户名\"\r\n                :rules=\"[{ required: true, message: '请输入用户名' }]\" />\r\n            <van-field v-model=\"password\" type=\"password\" name=\"password\" label=\"密码\" placeholder=\"密码\"\r\n                :rules=\"[{ required: true, message: '请输入密码' }]\" />\r\n            <div style=\"margin: 16px;\">\r\n                <van-button round block type=\"info\" native-type=\"submit\" color=\"#ffc400\">\r\n                    登录\r\n                </van-button>\r\n                <van-button round block type=\"info\" color=\"#ffc400\" class=\"register\" @click=\"toRegister\">\r\n                    去注册\r\n                </van-button>\r\n            </div>\r\n        </van-form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Header from '../../components/HeaderOf1.vue';\r\nimport { useRouter } from 'vue-router';\r\nimport { reactive,toRefs } from 'vue';\r\nimport {Toast} from 'vant';\r\nexport default {\r\n    components: {\r\n        Header,\r\n    },\r\n    setup(){\r\n        const router = useRouter();\r\n        const onSubmit = (value) => {\r\n            if(!localStorage.userInfo){\r\n                Toast('账号未注册！！！');\r\n                return;\r\n            }else{\r\n                let userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\r\n                if(userInfo['username'] === value['username']){\r\n                    if(userInfo['password'] === value['password']){\r\n                        localStorage.setItem(\"isLogin\",'login');\r\n                        Toast('登录成功!!!');\r\n                        router.push('/home');\r\n                    }else{\r\n                        Toast('密码错误!!!');\r\n                        return;\r\n                    }\r\n                }else{\r\n                    Toast('账号不存在!!!');\r\n                    return;\r\n                }\r\n            }\r\n        };\r\n        const toRegister = () => {\r\n            router.push('/register');\r\n        };\r\n        const data = reactive({\r\n            username: '',\r\n            password: '',\r\n        });\r\n        return{\r\n            onSubmit,\r\n            toRegister,\r\n            ...toRefs(data),\r\n        }\r\n    },\r\n};\r\n</scr