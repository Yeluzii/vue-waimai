{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Header from '../../components/HeaderOf1.vue';\nimport { useRouter } from 'vue-router';\nimport { reactive, toRefs } from 'vue';\nimport { Toast } from 'vant';\nexport default {\n  components: {\n    Header\n  },\n  setup() {\n    const router = useRouter();\n    const onSubmit = value => {\n      // console.log(value);\n      if (localStorage.userInfo) {\n        let userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\n        if (userInfo['username'] === value['username']) {\n          Toast('该用户名已存在');\n          return;\n        }\n      } else {\n        register(value);\n      }\n    };\n    const register = value => {\n      localStorage.setItem('userInfo', JSON.stringify(value));\n      Toast('注册成功');\n      router.push('/login');\n    };\n    const toLogin = () => {\n      router.push('/Login');\n    };\n    const data = reactive({\n      username: '',\n      password: ''\n    });\n    return {\n      onSubmit,\n      toLogin,\n      ...toRefs(data)\n    };\n  }\n};","map":{"version":3,"names":["Header","useRouter","reactive","toRefs","Toast","components","setup","router","onSubmit","value","localStorage","userInfo","JSON","parse","getItem","register","setItem","stringify","push","toLogin","data","username","password"],"sources":["D:\\vue3-projects\\vue-waimai\\src\\views\\register\\RegisterIndex.vue"],"sourcesContent":["<template>\r\n    <div class=\"register\">\r\n        <Header title=\"注册\" />\r\n        <div class=\"img\">米团</div>\r\n        <van-form @submit=\"onSubmit\">\r\n            <van-field v-model=\"username\" name=\"username\" label=\"用户名\" placeholder=\"用户名\"\r\n                :rules=\"[{ required: true, message: '请输入用户名' }]\" />\r\n            <van-field v-model=\"password\" type=\"password\" name=\"password\" label=\"密码\" placeholder=\"密码\"\r\n                :rules=\"[{ required: true, message: '请输入密码' }]\" />\r\n            <div style=\"margin: 16px;\">\r\n                <van-button round block type=\"info\" native-type=\"submit\" color=\"#ffc400\">\r\n                    注册\r\n                </van-button>\r\n                <van-button round block type=\"info\" color=\"#ffc400\" class=\"login\" @click=\"toLogin\">\r\n                    去登录\r\n                </van-button>\r\n            </div>\r\n        </van-form>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport Header from '../../components/HeaderOf1.vue';\r\nimport { useRouter } from 'vue-router';\r\nimport { reactive, toRefs } from 'vue';\r\nimport { Toast } from 'vant';\r\nexport default {\r\n    components: {\r\n        Header,\r\n    },\r\n    setup() {\r\n        const router = useRouter();\r\n        const onSubmit = (value) => {\r\n            // console.log(value);\r\n            if (localStorage.userInfo) {\r\n                let userInfo = JSON.parse(localStorage.getItem(\"userInfo\"));\r\n                if (userInfo['username'] === value['username']) {\r\n                    Toast('该用户名已存在');\r\n                    return;\r\n                }\r\n            } else {\r\n                register(value);\r\n            }\r\n        };\r\n        const register = (value) => {\r\n            localStorage.setItem('userInfo', JSON.stringify(value));\r\n            Toast('注册成功');\r\n            router.push('/login');\r\n        };\r\n        const toLogin = () => {\r\n            router.push('/Login');\r\n        };\r\n        const data = reactive({\r\n            username: '',\r\n            password: '',\r\n        });\r\n        return {\r\n            onSubmit,\r\n            toLogin,\r\n            ...toRefs(data),\r\n        }\r\n    },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.register {\r\n    .img {\r\n        width: 200px;\r\n        height: 200px;\r\n        background-color: #ffc400;\r\n        font-size: 80px;\r\n        line-height: 200px;\r\n        text-align: center;\r\n        border-radius: 40px;\r\n        margin: 20px auto;\r\n    }\r\n\r\n    .login {\r\n        margin-top: 20px;\r\n    }\r\n}\r\n</style>"],"mappings":";AAsBA,OAAOA,MAAK,MAAO,gCAAgC;AACnD,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,QAAQ,EAAEC,MAAK,QAAS,KAAK;AACtC,SAASC,KAAI,QAAS,MAAM;AAC5B,eAAe;EACXC,UAAU,EAAE;IACRL;EACJ,CAAC;EACDM,KAAKA,CAAA,EAAG;IACJ,MAAMC,MAAK,GAAIN,SAAS,CAAC,CAAC;IAC1B,MAAMO,QAAO,GAAKC,KAAK,IAAK;MACxB;MACA,IAAIC,YAAY,CAACC,QAAQ,EAAE;QACvB,IAAIA,QAAO,GAAIC,IAAI,CAACC,KAAK,CAACH,YAAY,CAACI,OAAO,CAAC,UAAU,CAAC,CAAC;QAC3D,IAAIH,QAAQ,CAAC,UAAU,MAAMF,KAAK,CAAC,UAAU,CAAC,EAAE;UAC5CL,KAAK,CAAC,SAAS,CAAC;UAChB;QACJ;MACJ,OAAO;QACHW,QAAQ,CAACN,KAAK,CAAC;MACnB;IACJ,CAAC;IACD,MAAMM,QAAO,GAAKN,KAAK,IAAK;MACxBC,YAAY,CAACM,OAAO,CAAC,UAAU,EAAEJ,IAAI,CAACK,SAAS,CAACR,KAAK,CAAC,CAAC;MACvDL,KAAK,CAAC,MAAM,CAAC;MACbG,MAAM,CAACW,IAAI,CAAC,QAAQ,CAAC;IACzB,CAAC;IACD,MAAMC,OAAM,GAAIA,CAAA,KAAM;MAClBZ,MAAM,CAACW,IAAI,CAAC,QAAQ,CAAC;IACzB,CAAC;IACD,MAAME,IAAG,GAAIlB,QAAQ,CAAC;MAClBmB,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACd,CAAC,CAAC;IACF,OAAO;MACHd,QAAQ;MACRW,OAAO;MACP,GAAGhB,MAAM,CAACiB,IAAI;IAClB;EACJ;AACJ,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}